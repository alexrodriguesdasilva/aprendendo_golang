/*
- Escreva expressões utilizando os operadores, e atribua seus valores a variáveis.
- Demonstre estes valores

- ==
- !=
- <=
- >=
- <
- >
*/

package main

import (
	"fmt"
)

func main() {
	a := (10 == 10)
	b := (10 != 10)
	c := (10 <= 10)
	d := (10 >= 10)
	e := (10 < 10)
	f := (10 > 10)

	fmt.Printf("O valor 10 é igual a 10?:\n%v\n\n", a)
	fmt.Printf("O valor 10 é diferente que 10?:\n%v\n\n", b)
	fmt.Printf("O valor 10 é menor ou igual a 10?:\n%v\n\n", c)
	fmt.Printf("O valor de 10 é maior ou igual a 10?:\n%v\n\n", d)
	fmt.Printf("O valor de 10 menor que 10:?\n%v\n\n", e)
	fmt.Printf("O valor de 10 maior que 10:?\n%v\n\n", f)
}